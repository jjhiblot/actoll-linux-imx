#include "imx6dl-sabresd.dts"

/ {
    model = "Freescale i.MX6 DualLite SABRE Smart Device Board";
    compatible = "fsl,imx6dl-sabresd", "fsl,imx6dl";

	aliases {
		can0 = &can1;
		can1 = &can2;
	};

	memory: memory@10000000 {
		reg = <0x10000000 0x40000000>;
	};

	/delete-node/ leds;
	/delete-node/ max8903@0;

	gpio-init {
		compatible = "gpio-init";
		init-gpios = <
			&gpio5 2 GPIO_ACTIVE_LOW
			&gpio5 1 0x0
			&gpio1 5 GPIO_ACTIVE_LOW
			&gpio5 9 0x0
			&gpio6 31 GPIO_ACTIVE_LOW
			&gpio5 20 0x0
			&gpio2 25 GPIO_ACTIVE_LOW>;
		reset-gpios = <&gpio3 22 GPIO_ACTIVE_LOW>;
		reset-delay-ms = <0x64>;
		#audio_en = <&gpio5 20 0x0>;
		#audio_vcc = <&gpio2 25 0x0>;
		status = "okay";
	};

	reg_2p5v: reg_2P5V {
		compatible = "regulator-fixed";
		regulator-name = "2P5V";
		regulator-min-microvolt = <2500000>;
		regulator-max-microvolt = <2500000>;
		regulator-always-on;
	};

	reg_3p3v: reg_3P3V {
		compatible = "regulator-fixed";
		regulator-name = "3P3V";
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		regulator-always-on;
	};

	gpio-keys {
		/delete-node/ volume-up;
		/delete-node/ volume-down;
	};

	/delete-node/ sound-hdmi;
	/delete-node/ sound;
	sound {
		compatible = "fsl,imx6q-sabresd-sgtl5000", "fsl,imx-audio-sgtl5000";
		model = "imx6q-sabresd-sgtl5000";
		ssi-controller = <&ssi1>;
		audio-codec = <&sgtl5000>;
		audio-routing =
			"MIC_IN", "Mic Jack",
			"Mic Jack", "Mic Bias",
			"Headphone Jack", "HP_OUT";
		mux-int-port = <1>;
		mux-ext-port = <4>;
	};


	lcd@0 {
		default_ifmt = "RGB24";
	};
};

&hdmi_audio {
	status = "disabled";
};
&hdmi_video {
	status = "disabled";
};
&epdc {
	status = "disabled";
};

&backlight_lvds {
	gpio-upbl = <&gpio1 0x4 0x1>; // JJH TODO: a quoi ca sert ?
	gpio-downbl = <&gpio1 0x8 0x1>; // JJH TODO: a quoi ca sert ?
};

&iomuxc {
	imx6qdl-sabresd {
		/delete-node/ uart5grp-1;
		/delete-node/ uart5dtegrp-1;
		/delete-node/ ipu1grp-2;
	};
};

&reg_usb_otg_vbus {
	/delete-property/ gpio;
};

&mxcfb1 {
	default_bpp = <32>;
};

&cpu0 {
	arm-supply = <&reg_arm>;
	soc-supply = <&reg_soc>;
};

&ecspi1 {
	status = "disable";
};

&i2c1 {
	/delete-node/ wm8962@1a;
	/delete-node/ ov564x@3c;
	/delete-node/ accelerometer@1c;

	sgtl5000: sgtl5000@a {
		compatible = "fsl,sgtl5000";
		reg = <0x0a>;
		clocks = <&clks IMX6QDL_CLK_CKO>;
		VDDA-supply = <&reg_2p5v>;
		VDDIO-supply = <&reg_3p3v>;
	};
};

&i2c2 {
	/delete-node/ ov564x_mipi@3c;
	/delete-node/ max11801@48;
	/delete-node/ touchscreen@4;
	/delete-node/ edid@50;

	rtc@51 {
		compatible = "phg,pcf8563";
		reg = <0x51>;
	};
};

&i2c3 {
	/delete-node/ egalax_ts@4;
	/delete-node/ magnetometer@e;
	/delete-node/ light-sensor@44;
	/delete-node/ elan@10;

	edid@50 {
		compatible = "fsl,imx6-hdmi-i2c";
		reg = <0x50>;
	};

	mcu@7d {
		compatible = "mxc,mcu";
		reg = <0x7D>;
	};

	st1633@55 {
		compatible = "sitronix,st1633";
		reg = <0x55>;
		interrupt-parent = <&gpio2>;
		interrupts = <0x18 0x2>;
		sitronix,irq-gpio = <&gpio2 0x18 0x0>;
		sitronix,rst-gpio = <&gpio3 0x17 0x0>;
		pinctrl-names = "default";
		pinctrl-0 = <&mx6_ft5x0x_pins>;
		status = "okay";
	};
	max17135@48 {
		compatible = "maxim,max17135";
		reg = <0x48>;
		vneg_pwrup = <0x1>;
		gvee_pwrup = <0x1>;
		vpos_pwrup = <0x2>;
		gvdd_pwrup = <0x1>;
		gvdd_pwrdn = <0x1>;
		vpos_pwrdn = <0x2>;
		gvee_pwrdn = <0x1>;
		vneg_pwrdn = <0x1>;
		SENSOR-supply = <0x2D>;
		gpio_pmic_pwrgood = <0x27 0x15 0x0>;
		gpio_pmic_vcom_ctrl = <0x4 0x11 0x0>;
		gpio_pmic_wakeup = <0x4 0x14 0x0>;
		gpio_pmic_v3p3 = <0x27 0x14 0x0>;
		gpio_pmic_intr = <0x27 0x19 0x0>;
		regulators {
			reg_display: DISPLAY {
				regulator-name = "DISPLAY";
			};
			GVDD {
				regulator-name = "GVDD";
			};
			GVEE {
				regulator-name = "GVEE";
			};
			HVINN {
				regulator-name = "HVINN";
			};
			HVINP {
				regulator-name = "HVINP";
			};
			reg_vcom: VCOM {
				regulator-name = "VCOM";
				regulator-min-microvolt = <0xFFBE0178>;
				regulator-max-microvolt = <0xFFF85EE0>;
			};
			VNEG {
				regulator-name = "VNEG";
			};
			VPOS {
				regulator-name = "VPOS";
			};
			reg_v3p3: V3P3 {
				regulator-name = "V3P3";
			};
		};
	};
};

&iomuxc {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_hog>;

	imx6qdl-sabresd {
		pinctrl_hog: hoggrp {
			fsl,pins = <
				MX6QDL_PAD_NANDF_D0__GPIO2_IO00		0x80000000
				MX6QDL_PAD_NANDF_D1__GPIO2_IO01		0x80000000
				MX6QDL_PAD_NANDF_D2__GPIO2_IO02		0x80000000
				MX6QDL_PAD_NANDF_D3__GPIO2_IO03		0x80000000
				MX6QDL_PAD_GPIO_0__CCM_CLKO1		0x130B0
				MX6QDL_PAD_NANDF_CLE__GPIO6_IO07	0x80000000
				MX6QDL_PAD_NANDF_ALE__GPIO6_IO08	0x80000000
				MX6QDL_PAD_ENET_TXD1__GPIO1_IO29	0x80000000
				MX6QDL_PAD_EIM_D21__GPIO3_IO21		0x80000000
				MX6QDL_PAD_EIM_D22__GPIO3_IO22		0x80000000
				MX6QDL_PAD_ENET_CRS_DV__GPIO1_IO25	0x80000000
				MX6QDL_PAD_ENET_RXD0__GPIO1_IO27	0x80000000
				MX6QDL_PAD_EIM_A25__GPIO5_IO02		0x80000000
				MX6QDL_PAD_EIM_D23__GPIO3_IO23		0x80000000
				MX6QDL_PAD_SD3_RST__GPIO7_IO08		0x80000000
				MX6QDL_PAD_EIM_DA9__GPIO3_IO09		0x80000000
				MX6QDL_PAD_NANDF_CS0__GPIO6_IO11	0x80000000
				MX6QDL_PAD_NANDF_CS1__GPIO6_IO14	0x80000000
				MX6QDL_PAD_CSI0_DATA_EN__GPIO5_IO20	0x80000000
				MX6QDL_PAD_EIM_OE__GPIO2_IO25		0x80000000
				MX6QDL_PAD_SD1_DAT0__GPIO1_IO16		0x80000000
				MX6QDL_PAD_SD1_DAT1__GPIO1_IO17		0x80000000
				MX6QDL_PAD_SD1_CLK__GPIO1_IO20		0x80000000
				MX6QDL_PAD_SD1_DAT2__GPIO1_IO19		0x80000000
				MX6QDL_PAD_EIM_CS0__GPIO2_IO23		0x80000000
				MX6QDL_PAD_EIM_EB2__GPIO2_IO30		0x80000000
				MX6QDL_PAD_KEY_COL4__GPIO4_IO14		0x80000000
				MX6QDL_PAD_EIM_D16__GPIO3_IO16		0x80000000
				MX6QDL_PAD_KEY_ROW2__GPIO4_IO11		0x80000000
				MX6QDL_PAD_EIM_D18__GPIO3_IO18		0x80000000
				MX6QDL_PAD_GPIO_9__GPIO1_IO09		0x80000000
				MX6QDL_PAD_EIM_D17__GPIO3_IO17		0x80000000
				MX6QDL_PAD_GPIO_17__GPIO7_IO12		0x80000000
				MX6QDL_PAD_GPIO_4__GPIO1_IO04		0x80000000
				MX6QDL_PAD_KEY_COL2__GPIO4_IO10		0x80000000
				MX6QDL_PAD_GPIO_8__GPIO1_IO08		0x80000000
				MX6QDL_PAD_EIM_BCLK__GPIO6_IO31		0x80000000
			>;
		};

		pinctrl_audmux: audmuxgrp {
			fsl,pins = <
				MX6QDL_PAD_SD2_DAT0__AUD4_RXD 0x130B0
				MX6QDL_PAD_SD2_DAT1__AUD4_TXFS 0x130B0
				MX6QDL_PAD_SD2_DAT2__AUD4_TXD 0x110B0
				MX6QDL_PAD_SD2_DAT3__AUD4_TXC 0x130B0
			>;
		};

		pinctrl_ecspi1: ecspi1grp {
			fsl,pins;
		};

		pinctrl_enet: enetgrp {
			fsl,pins = <
				MX6QDL_PAD_ENET_MDIO__ENET_MDIO		0x1b0b0
				MX6QDL_PAD_ENET_MDC__ENET_MDC		0x1b0b0
				MX6QDL_PAD_RGMII_TXC__RGMII_TXC		0x1b030
				MX6QDL_PAD_RGMII_TD0__RGMII_TD0		0x1b030
				MX6QDL_PAD_RGMII_TD1__RGMII_TD1		0x1b030
				MX6QDL_PAD_RGMII_TD2__RGMII_TD2		0x1b030
				MX6QDL_PAD_RGMII_TD3__RGMII_TD3		0x1b030
				MX6QDL_PAD_RGMII_TX_CTL__RGMII_TX_CTL	0x1b030
				MX6QDL_PAD_ENET_REF_CLK__ENET_TX_CLK	0x1A0B1
				MX6QDL_PAD_RGMII_RXC__RGMII_RXC		0x1b030
				MX6QDL_PAD_RGMII_RD0__RGMII_RD0		0x1b030
				MX6QDL_PAD_RGMII_RD1__RGMII_RD1		0x1b030
				MX6QDL_PAD_RGMII_RD2__RGMII_RD2		0x1b030
				MX6QDL_PAD_RGMII_RD3__RGMII_RD3		0x1b030
				MX6QDL_PAD_RGMII_RX_CTL__RGMII_RX_CTL	0x1b030
				MX6QDL_PAD_GPIO_16__ENET_REF_CLK	0x4001b0a8
			>;
		};

		pinctrl_gpio_keys: gpio_keysgrp {
			fsl,pins = <
				MX6QDL_PAD_EIM_D29__GPIO3_IO29 0x1b0b0
				MX6QDL_PAD_GPIO_5__GPIO1_IO05  0x1b0b0
			>;
		};

		pinctrl_hdmi_cec: hdmicecgrp {
			fsl,pins;
		};

		pinctrl_ipu1_2: ipu1grp-2 { /* parallel camera */
			fsl,pins = <
				MX6QDL_PAD_CSI0_DAT12__IPU1_CSI0_DATA12    0x80000000
				MX6QDL_PAD_CSI0_DAT13__IPU1_CSI0_DATA13    0x80000000
				MX6QDL_PAD_CSI0_DAT14__IPU1_CSI0_DATA14    0x80000000
				MX6QDL_PAD_CSI0_DAT15__IPU1_CSI0_DATA15    0x80000000
				MX6QDL_PAD_CSI0_DAT16__IPU1_CSI0_DATA16    0x80000000
				MX6QDL_PAD_CSI0_DAT17__IPU1_CSI0_DATA17    0x80000000
				MX6QDL_PAD_CSI0_DAT18__IPU1_CSI0_DATA18    0x80000000
				MX6QDL_PAD_CSI0_DAT19__IPU1_CSI0_DATA19    0x80000000
				MX6QDL_PAD_CSI0_PIXCLK__IPU1_CSI0_PIXCLK   0x80000000
				MX6QDL_PAD_CSI0_MCLK__IPU1_CSI0_HSYNC      0x80000000
				MX6QDL_PAD_CSI0_VSYNC__IPU1_CSI0_VSYNC     0x80000000
			>;
		};

		pinctrl_uart1: uart1grp {
			fsl,pins = <
				MX6QDL_PAD_SD3_DAT7__UART1_TX_DATA	0x1B0B1
				MX6QDL_PAD_SD3_DAT6__UART1_RX_DATA	0x1B0B1
			>;
		};

		uart2grp: uart2grp {
			fsl,pins = <
				MX6QDL_PAD_EIM_D26__UART2_TX_DATA 0x1B0B1
				MX6QDL_PAD_EIM_D27__UART2_RX_DATA 0x1B0B1
				>;
		};
		uart3grp: uart3grp {
			fsl,pins = <
				MX6QDL_PAD_EIM_D24__UART3_TX_DATA 0x1B081
				MX6QDL_PAD_EIM_D25__UART3_RX_DATA 0x1B081
				>;
		};
		uart4grp: uart4grp {
			fsl,pins = <
				MX6QDL_PAD_KEY_COL0__UART4_TX_DATA 0x1B0B1
				MX6QDL_PAD_KEY_ROW0__UART4_RX_DATA 0x1B0B1
			>;
		};
		uart5grp: uart5grp {
			fsl,pins = <
				MX6QDL_PAD_KEY_COL1__UART5_TX_DATA 0x1B0B1
				MX6QDL_PAD_KEY_ROW1__UART5_RX_DATA 0x1B0B1
			>;
		};

		pinctrl_usdhc3: usdhc3grp {
			fsl,pins = <
				MX6QDL_PAD_SD3_CMD__SD3_CMD		0x17059
				MX6QDL_PAD_SD3_CLK__SD3_CLK		0x10059
				MX6QDL_PAD_SD3_DAT0__SD3_DATA0	0x17059
				MX6QDL_PAD_SD3_DAT1__SD3_DATA1	0x17059
				MX6QDL_PAD_SD3_DAT2__SD3_DATA2	0x17059
				MX6QDL_PAD_SD3_DAT3__SD3_DATA3	0x17059
				MX6QDL_PAD_SD3_DAT5__GPIO7_IO00	0x80000000
			>;
		};

		flexcan1grp: flexcan1grp {
			fsl,pins = <MX6QDL_PAD_GPIO_7__FLEXCAN1_TX 0x80000000>;
		};
		flexcan2grp: flexcan2grp {
			fsl,pins = <MX6QDL_PAD_KEY_ROW4__FLEXCAN2_RX 0x80000000>;
		};
		mx6_ft5x0x_pins: mx6_ft5x0x_pins {
			fsl,pins = <MX6QDL_PAD_EIM_CS1__GPIO2_IO24 0x80000000>;
		};
	};
};

&wdog1 {
    status = "okay";
};

&wdog2 {
    status = "disabled";
};

&gpc {
	fsl,ldo-bypass = <0>;
};

&ipu1_csi1_from_ipu1_csi1_mux {
	clock-lanes = <0>;
	data-lanes = <1 2>;
};

&ldb {
	dual-mode;

	status = "okay";

	lvds-channel@0 {
		fsl,data-mapping = "spwg";
		fsl,data-width = <18>;
		crtc = "ipu1-di0";
		primary;
		status = "okay";

		display-timings {
			native-mode = <&timing0>;
			timing0: hsd100pxn1 {
				clock-frequency = <0x1fc1e20>;
				hactive = <0x320>;
				vactive = <0x1e0>;
				hback-porch = <0x2e>;
				hfront-porch = <0xd2>;
				vback-porch = <0x17>;
				vfront-porch = <0x16>;
				hsync-len = <0x01>;
				vsync-len = <0x01>;
			};
		};
	};
	lvds-channel@1 {
		reg = <0x01>;
		status = "disabled";
		crtc = "ipu1-di1";
	};
};

&mipi_dsi {
	status = "disabled";
};

&uart1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_uart1>;
	status = "okay";
};

&usdhc2 {
	status = "disabled";
};

&usdhc3 {
	bus-width = <0x4>;
	cd-gpios = <&gpio7 0 0>;
	/delete-property/ wp-gpios;
};

&uart5 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&uart5grp>;
};

&uart4{
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&uart4grp>;
};

&uart3 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&uart3grp>;
};

&uart2 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&uart2grp>;
};

&can1 {
	pinctrl-names = "default";
	pinctrl-0 = <&flexcan1grp>;

	status = "okay";
};

&can2 {
	pinctrl-names = "default";
	pinctrl-0 = <&flexcan2grp>;

	status = "okay";
};

&ssi1 {
	status = "okay";
	fsl,mode = "i2s-slave";
};



&reg_arm {
       /delete-property/ vin-supply;
};

&reg_pu {
       /delete-property/ vin-supply;
};

&reg_soc {
       /delete-property/ vin-supply;
};

&reg_vdd1p1 {
	/delete-property/ vin-supply;
};

&reg_vdd2p5 {
	/delete-property/ vin-supply;
};

&reg_usb_h1_vbus {
	/delete-property/ vin-supply;
};

&reg_usb_otg_vbus {
	/delete-property/ vin-supply;
};
